{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "description": "A simple portfolio demonstrating some of my React JS skillsets",
  "engines": {
    "node": "15.2.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "lint": "eslint --fix ./src/*/*.tsx --color",
    "test": "env TS_NODE_PROJECT='tsconfig.test.json' mocha --require ts-node/register --verbose --config './test/.mocharc.js' --recursive './test/**/*/*.test.tsx' --exit",
    "tint": "npm-run-all lint test"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.2.2",
    "@types/axios": "^0.14.0",
    "@types/chai": "^4.2.14",
    "@types/chai-enzyme": "^0.6.8",
    "@types/enzyme": "^3.10.8",
    "@types/mocha": "^8.2.0",
    "@types/node": "^12.12.47",
    "@types/react": "^17.0.0",
    "@types/sinon-chai": "^3.2.5",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "axios": "^0.21.1",
    "chai": "^4.2.0",
    "chai-enzyme": "^1.0.0-beta.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint": "^7.17.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-react": "^7.22.0",
    "mocha": "^8.2.1",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.1",
    "react-bootstrap": "^1.4.3",
    "react-dom": "^17.0.1",
    "react-refresh": "^0.9.0",
    "react-scripts": "^4.0.1",
    "sinon": "^9.2.3",
    "sinon-chai": "^3.5.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.2"
  }
}
